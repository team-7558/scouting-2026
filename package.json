{
  "name": "scouting-2023-server",
  "version": "1.0.0",
  "description": "Server for the scouting-2023 off season app",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npm run build --prefix client && npm run server",
    "server": "nodemon --quiet server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "path": "^0.12.7",
    "react-scripts": "^5.0.1"
  },
  "overrides": {
    "react-scripts": {
      "@svgr/webpack": "8.1.0",
      "typescript": "4.9.5",
      "postcss": "8.4.38"
    }
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.5"
  }
}
